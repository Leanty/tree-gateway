{
    "swagger": {
        "yaml": false,
        "outputDirectory": "./dist/admin/api",
        "entryFile": "./src/admin/api/admin-api.ts",
        "name": "Tree Gateway Admin API",
        "description": "The administrative API for Tree Gateway",
        "license": "MIT",
        "basePath": "/",
        "securityDefinitions": {
            "Bearer": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        },
        "produces": ["application/json"],
        "version": "1.1.4"
    }
}
