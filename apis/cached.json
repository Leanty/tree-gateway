{
    "name": "TestCachedAPI",
    "version": "1.0.0",
    "group": [
        {
            "name": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/"], 
                    "method": ["GET"]
                } 
            ] 
        }
    ],
    "proxy": {
        "path": "testCache/",
        "target": {
            "path": "http://httpbin.org"
        },
        "preserveHostHdr": true,
        "timeout": 5000
    },
    "cache": {
        "client": {
            "cacheTime": "10 minutes",
            "cacheControl": "public",
            "mustRevalidate": false,
            "noTransform": false,
            "proxyRevalidate": false
        },
        "server": {

        },
        "group": ["Group1"]
    }
}