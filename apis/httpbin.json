{
    "name": "Test API",
    "version": "1.0.0",
    "proxy": {
        "path": "test/",
        "target": {
            "path": "http://httpbin.org",
            "allow": {
                "path": ["get/", "post/"],
                "method": ["get"]
            },
            "deny": {
                "path": ["user-agent"],
                "method": ["post"]
            }
        },
        "filter": [
        ],
        "preserveHostHdr": true,
        "timeout": 5000
    }
}