{
    "name": "TestInterceptedAPI",
    "version": "1.0.0",
    "proxy": {
        "path": "/intercepted",
        "target": {
            "path": "http://httpbin.org"
        },
        "preserveHostHdr": true,
        "timeout": 5000,
        "interceptor": {
            "request": [
                {"name":"myRequestInterceptor", "appliesTo": ["get/"]},
                {"name":"mySecondRequestInterceptor"}
            ], 
            "response": [
                {"name":"myResponseInterceptor"}, 
                {"name":"SecondInterceptor", "appliesTo": ["get/"]}
            ] 
        }
    }
}