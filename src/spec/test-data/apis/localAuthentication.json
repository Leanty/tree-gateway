{
    "name": "TestLocalAuthenticatedAPI",
    "version": "1.0.0",
    "proxy": {
        "path": "secureLocal/",
        "target": {
            "path": "http://httpbin.org"
        },
        "timeout": 5000
    },
    "authentication": {
        "strategy": {
            "local": {
                "usernameField": "userid",
                "passwordField": "passwd",
                "verify": "verifyBasicUser"
            }
        }
    }
}