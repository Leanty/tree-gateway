{
    "name": "TestCustomAuthenticationAPI",
    "version": "1.0.0",
    "proxy": {
        "path": "secureCustom/",
        "target": {
            "host": "http://httpbin.org"
        },
        "timeout": 5000
    },
    "authentication": {
        "strategy": {
            "myJwtStrategy": {
                "secret": "secret"
            }
        }
    }
}