{
    "name": "TestFilteredAPI",
    "version": "1.0.0",
    "group": [
        {
            "name": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/", "/user-agent"],
                    "method": ["GET"]
                },
                {
                    "path": ["headers/"] 
                }
            ] 
        }
    ],
    "proxy": {
        "path": "filtered/",
        "target": {
            "path": "http://httpbin.org"
        },
        "filter": [
            {
                "name": "myCustomFilter",
                "group": ["Group1"]
            },
            {
                "name": "mySecondFilter", 
                "group": ["Group1"]
            }
        ],
        "preserveHostHdr": true,
        "timeout": 5000
    }
}