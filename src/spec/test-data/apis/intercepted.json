{
    "name": "TestInterceptedAPI",
    "version": "1.0.0",
    "group": [
        {
            "name": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/", "/test"],
                    "method": ["GET"]
                },
                {
                    "path": ["anotherPath/"] 
                }
            ] 
        }
    ],
    "proxy": {
        "path": "/intercepted",
        "target": {
            "path": "http://httpbin.org"
        },
        "timeout": 5000,
        "interceptor": {
            "request": [
                {"name":"myRequestInterceptor", "group": ["Group1"]},
                {"name":"mySecondRequestInterceptor"}
            ], 
            "response": [
                {"name":"myResponseInterceptor"}, 
                {"name":"SecondInterceptor", "group": ["Group1"]}
            ] 
        }
    }
}