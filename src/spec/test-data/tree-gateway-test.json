{
    "underProxy": false,
    "listenPort": 8000,
    "adminPort": 8001,
    "rootPath": "./temp",
    "logger": {
        "level": "error",
        "file": {
            "timestamp": true,
            "outputDir": "./logs",
            "json": false, 
            "prettyPrint": true 
        }
    },
    "statsConfig": {
        "granularity": {
            "duration": "1 hour",
            "ttl": "2 days"
        }
    }, 
    "database": {
        "standalone": {
            "host": "localhost",
            "port": 6379
        },
        "options": {
            "db": 1
        }
    }
}
