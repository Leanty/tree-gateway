{
    "name": "Test Filtered API",
    "version": "1.0.0",
    "proxy": {
        "path": "filtered/",
        "target": {
            "path": "http://httpbin.org"
        },
        "filter": [
            {
                "name": "myCustomFilter"
            },
            {
                "name": "mySecondFilter", 
                "appliesTo": ["get/"]
            }
        ],
        "preserveHostHdr": true,
        "timeout": 5000
    },
    "throttling": {
        
    },
    "serviceDiscovery": {

    }
}