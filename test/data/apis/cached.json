{
    "name": "TestCachedAPI",
    "version": "1.0.0",
    "path": "testCache/",
    "group": [
        {
            "id": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/"], 
                    "method": ["GET"]
                } 
            ] 
        }
    ],
    "proxy": {
        "target": {
            "host": "http://localhost:7000"
        },
        "timeout": 5000
    },
    "cache": [
        {
            "use": "my-cache",
            "group": ["Group1"]
        }
    ]
}