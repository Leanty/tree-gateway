{
    "name": "TestCustomAuthenticationAPI",
    "version": "1.0.0",
    "path": "secureCustom/",
    "proxy": {
        "target": {
            "host": "http://httpbin.org"
        },
        "timeout": 5000
    },
    "authentication": {
        "strategy": {
            "id": "myJwtStrategy",
            "options": {
                "secret": "secret"
            }
        }
    },
    "errorHandler": {
        "use": "my-ErrorHandler" 
    }
}