{
    "name": "TestFilteredAPI",
    "version": "1.0.0",
    "path": "filtered/",
    "group": [
        {
            "id": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/", "/user-agent"],
                    "method": ["GET"]
                },
                {
                    "path": ["headers/"] 
                }
            ] 
        }
    ],
    "proxy": {
        "target": {
            "host": "http://httpbin.org"
        },
        "filter": [
            {
                "middleware": {
                    "name": "myCustomFilter"
                },
                "group": ["Group1"]
            },
            {
                "middleware": {
                    "name": "mySecondFilter"
                },
                "group": ["Group1"]
            }
        ],
        "timeout": 5000
    }
}