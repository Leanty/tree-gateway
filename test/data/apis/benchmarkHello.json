{
    "name": "BenchmarkHelloAPI",
    "version": "1.0.0",
    "path": "benchmark/hello",
    "proxy": {
        "target": {
            "host": "http://localhost:9000"
        },
        "httpAgent": {
            "keepAlive": true,
            "keepAliveTime": "one second",
            "freeSocketKeepAliveTimeout": "30 seconds",
            "maxFreeSockets": 10,
            "maxSockets": 200,
            "timeout": "one minute"
        },
        "timeout": "five seconds", 
        "parseReqBody": false, 
        "disableStats": true
    }
}