{
    "name": "TestBasicAuthenticatedAPI",
    "version": "1.0.0",
    "path": "secureBasic/",
    "proxy": {
        "target": {
            "host": "http://localhost:7000"
        },
        "timeout": 5000
    },
    "authentication": {
        "strategy": {
            "name": "basic",
            "options": {
                "verify": {
                    "name": "verifyBasicUser"
                }
            }
        }
    }
}