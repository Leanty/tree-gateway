{
    "name": "TestLimitByGroupAPI",
    "version": "1.0.0",
    "path": "limited-by-group",
    "group": [
        {
            "id": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/", "/user-agent/**"],
                    "method": ["GET"]
                }
            ] 
        }
    ],
    "proxy": {
        "target": {
            "host": "http://localhost:7000"
        },
        "timeout": 5000
    },
    "throttling": [
        {
            "use": "my-rate-limit",
            "group": ["Group1"]
        }
    ]
}