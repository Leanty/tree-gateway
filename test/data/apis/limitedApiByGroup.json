{
    "name": "TestLimitByGroupAPI",
    "version": "1.0.0",
    "path": "limited-by-group",
    "group": [
        {
            "id": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/", "/user-agent"],
                    "method": ["GET"]
                }
            ] 
        }
    ],
    "proxy": {
        "target": {
            "host": "http://httpbin.org"
        },
        "timeout": 5000
    },
    "throttling": [
        {
            "timeWindow": "one minute",
            "delay": "1 second",
            "max": 1,
            "group": ["Group1"]        
        }
    ],
    "serviceDiscovery": {

    }
}