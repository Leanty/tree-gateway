{
    "name": "TestInterceptedAPI",
    "version": "1.0.0",
    "path": "/intercepted",
    "group": [
        {
            "id": "Group1",
            "description": "Endpoints Group One",
            "member": [
                {
                    "path": ["get/", "/test"],
                    "method": ["GET"]
                },
                {
                    "path": ["anotherPath/"] 
                }
            ] 
        }
    ],
    "proxy": {
        "target": {
            "host": "http://httpbin.org"
        },
        "timeout": 5000,
        "interceptor": {
            "request": [
                {
                    "middleware": {
                        "id":"myRequestInterceptor"
                    }, 
                    "group": ["Group1"]
                },
                {
                    "middleware": {"name":"mySecondRequestInterceptor"}
                }
            ], 
            "response": [
                {
                    "middleware": {"id":"myResponseInterceptor"}
                }, 
                {
                    "middleware": {
                        "name":"SecondInterceptor"
                    },
                    "group": ["Group1"]
                }
            ] 
        }
    }
}